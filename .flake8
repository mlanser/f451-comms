[flake8]
select = B,B9,C,D,DAR,E,F,N,RST,S,W
ignore = E203,E501,RST201,RST203,RST301,W503,D104,D105,D107,D402,N801,N802,N803,N806,N812, N816
max-line-length = 99
max-complexity = 10
docstring-convention = google
per-file-ignores = tests/*:S101
rst-roles = class,attr,const,func,meth,mod,ref
rst-directives = deprecated
